@model StudentCourses
<label>@Model.StudentDetails.Id</label>
<label>@Model.StudentDetails.Name</label>
<label>@Model.StudentDetails.GPA</label>
<table class="table">
    <thead>
    <tr>
        <th>Course ID</th>
        <th>Name</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var course in Model.Courses)
        {
            <tr>
                <td>@course.Id</td>
                <td>@course.Name</td>
            </tr>
        }
    </tbody>
</table>