@model List<Course>

<table class="table">
    <thead>
    <tr>
        <th>Course ID</th>
        <th>Name</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td>@course.Id</td>
                <td>@course.Name</td>
            </tr>
        }
    </tbody>
</table>